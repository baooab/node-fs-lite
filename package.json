  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.4.2",
    "@vitest/coverage-v8": "^0.33.0",
    "@vitest/ui": "^0.33.0",
    "unbuild": "^1.2.1",
    "vitest": "^0.33.0"
  }
